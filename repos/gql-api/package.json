{
  "name": "gql-api",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node ./bin/www",
    "dev": "SET NODE_ENV=development& nodemon --exec babel-node ./bin/www",
    "build": "mkdir deploy & babel index.js -s -d deploy",
    "clean": "rd /s /q deploy",
    "rebuild": "npm run clean & npm run build"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.5.0",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "nodemon": "^2.0.2"
  },
  "author": "Ahmet Egesel",
  "license": "MIT"
}
