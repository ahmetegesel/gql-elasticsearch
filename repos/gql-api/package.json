{
  "name": "gql-api",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node ./index.js",
    "dev": "SET NODE_ENV=development & nodemon --exec babel-node ./bin/www",
    "build": "mkdir deploy & babel index.js -s -d deploy",
    "clean": "rd /s /q deploy",
    "rebuild": "npm run clean & npm run build"
  },
  "dependencies": {
    "apollo-server": "^2.10.1",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.6.0",
    "express": "~4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "babel-plugin-import-graphql": "^2.7.0",
    "nodemon": "^2.0.2"
  },
  "author": "Ahmet Egesel",
  "license": "MIT"
}
